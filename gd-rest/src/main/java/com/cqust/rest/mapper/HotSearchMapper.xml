<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cqust.rest.mapper.HotSearchMapper">

	<select id="geTHotsearchs" resultType="com.cqust.pojo.THotsearch">
		SELECT
		*
		FROM
		t_hotsearch
		order by hotCount desc
	</select>


</mapper>